CRATE DATABASE TABLERO;

CREATE TABLE expensive (id VARCHAR(50), description VARCHAR(120), PRIMARY KEY(id));
CREATE TABLE tag (id VARCHAR(50), description VARCHAR(120), PRIMARY KEY(id));
CREATE TABLE user (id INT(10) NOT NULL AUTO_INCREMENT, name VARCHAR(120), salary INT(10), PRIMARY KEY(id));
CREATE TABLE logs(id INT(10) NOT NULL AUTO_INCREMENT, expensive_id VARCHAR(50), tag_id VARCHAR(50), date DATETIME, PRIMARY KEY(id));
